#3
with open ("konto.txt" , "r" )as fail:
    read = fail.readlines()
for rida in read:
    arv = float(rida.strip())
    if arv > 0:
        print(arv)
fail.close()



#4
valik = input("Sisseta faili nimi: ")
fail = open("valik")
read = fail.readlines()
for i in read:
    print(1, i, end= '') 







